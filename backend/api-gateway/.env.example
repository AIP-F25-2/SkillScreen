# API Gateway Environment Variables
FLASK_ENV=development
FLASK_DEBUG=False
SECRET_KEY=your-secret-key-here
JWT_SECRET_KEY=your-jwt-secret-key-here

# Service URLs - Core Infrastructure (5000-5009)
USER_SERVICE_URL=http://user-service:5001
AUTH_SERVICE_URL=http://auth-service:5002
INTERVIEW_SERVICE_URL=http://interview-service:5003
MEDIA_SERVICE_URL=http://media-service:5004
ASSESSMENT_SERVICE_URL=http://assessment-service:5005
NOTIFICATION_SERVICE_URL=http://notification-service:5006
LOGGER_SERVICE_URL=http://logger-service:5007

# AI Services (5010-5019)
TEXT_AI_SERVICE_URL=http://text-ai-service:5010
AUDIO_AI_SERVICE_URL=http://audio-ai-service:5011
VIDEO_AI_SERVICE_URL=http://video-ai-service:5012

# Specialized Services (5020+)
CODING_SERVICE_URL=http://coding-service:5020

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100
RATE_LIMIT_BURST=20

# Server Configuration
PORT=5000
